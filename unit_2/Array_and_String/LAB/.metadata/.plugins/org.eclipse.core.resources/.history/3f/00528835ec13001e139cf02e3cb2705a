#include <stdio.h>

int main(void)
{
	int rows, columns;
	printf("Enter the number of rows");
	fflush(stdin);  fflush(stdout);
	scanf("%d",&rows);
	printf("Enter the number of columns");
	fflush(stdin);  fflush(stdout);
	scanf("%d",&columns);
	int matrix[rows][columns];
	/////////////////////////////////////
	printf("Enter the element\n");
	fflush(stdin);  fflush(stdout);
	for(int i=0;i<rows;i++)
	{
		for(int k=0;k<columns;k++)
		{
			scanf("%d",&matrix[i][k]);
		}
	}
	for(int i=0;i<rows;i++)
	{
		for(int k=0;k<columns;k++)
		{
			printf("%d ",matrix[i][k]);
		}
		printf("\n");
	}

}
